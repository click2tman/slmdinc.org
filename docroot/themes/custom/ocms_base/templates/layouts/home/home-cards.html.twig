                content.fourth_row_col_1 is empty and content.fourth_row_col_2 is empty and content.fourth_row_col_3 ? 'col-xs-12',
                content.fourth_row_col_1 and content.fourth_row_col_2 is empty and content.fourth_row_col_3 is empty ? 'col-xs-12',
                content.fourth_row_col_1 is empty and content.fourth_row_col_2 and content.fourth_row_col_3 is empty ? 'col-xs-12',
                content.fourth_row_col_1 is empty and content.fourth_row_col_2 is empty and content.fourth_row_col_3 is empty ? 'hidden',
            ]
            %}
        
            {% if content.fourth_row_col_1 %}
