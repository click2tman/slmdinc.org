{#
/**
 * @file
 * OCMS HomeTemplate for a 4 row panel layout with 5 optional content rows
 *
 * This template provides a 4 row panel display layout, with 5 optional content rows
 *
 * Variables:
 * - $id: An optional CSS id to use for the layout.
 * - $content: An array of content, each item in the array is keyed to one
 *   panel of the layout. This layout supports the following sections:
 *   - content.first_row: Content in the top row.
 *   - content.second_row: Content in the second row. 
 *   - content.third_row_col_1: Content in the third row, column 1
 *   - content.third_row_col_2_top: Content in the third row, column 2, top
 *   - content.third_row_col_2_bottom: Content in the third row, column 2, bottom
 *   - content.fourth_row_col_1: Content in the fourth row, column 1.
 *   - content.fourth_row_col_2: Content in the fourth row, column 2.
 *   - content.fourth_row_col_3: Content in the fourth row, column 3.
 *   - 5 optional content rows. 
 */
#}
  {{ content.field_ocms_media_image }}
<div class="ocms-home-cards">
    {# First Row #}
    {% if content.first_row %}
    <div class="row">
        <div class="col-md-12 ocms-home-cards__first">
            {{ content.first_row }}
        </div>
    </div>
    {% endif %}  {# First Row Ends #}

    {# Second Row #}
    {% if content.second_row %}
    <div class="row">
        <div class="col-md-12 ocms-home-cards__second">
            {{ content.second_row }}
        </div>
    </div>
    {% endif %}  {# Second Row Ends #}

    {# Third Row #}
    {% if (content.third_row_col_1 or content.third_row_col_2_top or content.third_row_col_2_bottom) %}
    <div class="row">
        <div class="ocms-home-cards__third">
            {%
            set third_row_col_1_class = [
                content.third_row_col_1 and content.third_row_col_2_top and content.third_row_col_2_bottom ? 'col-xs-12 col-sm-6',
                content.third_row_col_1 and content.third_row_col_2_top is empty and content.third_row_col_2_bottom ? 'col-xs-12 col-sm-6',
                content.third_row_col_1 and content.third_row_col_2_top and content.third_row_col_2_bottom is empty ? 'col-xs-12 col-sm-6',
                content.third_row_col_1 is empty ? 'hidden'
            ]
            %}
            {%
            set third_row_col_2_class = [
                content.third_row_col_1 and content.third_row_col_2_top and content.third_row_col_2_bottom ? 'col-xs-12 col-sm-6',
                content.third_row_col_1 and content.third_row_col_2_top is empty and content.third_row_col_2_bottom ? 'col-xs-12 col-sm-6',
                content.third_row_col_1 and content.third_row_col_2_top and content.third_row_col_2_bottom is empty ? 'col-xs-12 col-sm-6',
                content.third_row_col_1 is empty and content.third_row_col_2_top and content.third_row_col_2_bottom ? 'col-xs-12',
                content.third_row_col_1 is empty and content.third_row_col_2_top is empty and content.third_row_col_2_bottom ? 'col-xs-12',
                content.third_row_col_1 is empty and content.third_row_col_2_top and content.third_row_col_2_bottom is empty ? 'col-xs-12',
                content.third_row_col_2_top is empty and content.third_row_col_2_bottom is empty ? 'hidden'
            ]
            %}
          
            {% if content.third_row_col_1 %}
                <div{{ create_attribute({'class': third_row_col_1_class}) }}>
                    {{ content.third_row_col_1 }}
                </div>
            {% endif %}

            {% if content.third_row_col_2_top or content.third_row_col_2_bottom %}
                <div{{ create_attribute({'class': third_row_col_2_class}) }}>
                    <div class="row">
                        <div class="col-xs-12">
                            {{ content.third_row_col_2_top }}
                        </div>
                        <div class="col-xs-12">
                            {{ content.third_row_col_2_bottom }}
                        </div>
                    </div>
                </div>
            {% endif %}

        </div>
    </div>
    {% endif %}  {# Third Row Ends #}

    {# Fourth Row #}
    {% if (content.fourth_row_col_1 or content.fourth_row_col_2 or content.fourth_row_col_3) %}
    <div class="row">
        <div class="ocms-home-cards__fourth">
            {%
            set fourth_row_class = [
                content.fourth_row_col_1 and content.fourth_row_col_2 and content.fourth_row_col_3 ? 'col-xs-12 col-sm-4',
                content.fourth_row_col_1 is empty and content.fourth_row_col_2 and content.fourth_row_col_3 ? 'col-xs-12 col-sm-6',
